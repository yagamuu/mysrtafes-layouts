{
  "name": "mysrtafes-layouts",
  "version": "1.0.0",
  "description": "The on-stream layouts for Mystery Dungeon RTA FES",
  "homepage": "https://oengus.io/marathon/mysrtafes",
  "bugs": {
    "url": "https://github.com/yagamuu/mysrtafes-layouts/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yagamuu/mysrtafes-layouts.git"
  },
  "license": "MIT",
  "author": "zoton2",
  "scripts": {
    "autofix": "run-s autofix:*",
    "autofix:browser": "eslint --fix --ext .ts,.vue src/dashboard && eslint --fix --ext .ts,.vue src/graphics && eslint --fix --ext .ts src/browser_shared",
    "autofix:extension": "eslint --fix --ext .ts src/extension",
    "build": "run-s build:*",
    "build:browser": "cross-env NODE_ENV=production webpack",
    "build:extension": "tsc -b tsconfig.extension.json",
    "clean": "trash node_modules/.cache && trash dashboard && trash graphics && trash extension",
    "start": "node ../..",
    "typeschemas": "node script/typeschemas.js",
    "watch": "run-p watch:*",
    "watch:browser": "webpack -w",
    "watch:extension": "tsc -b tsconfig.extension.json -w"
  },
  "devDependencies": {
    "@mdi/font": "^4.8.95",
    "@types/clone": "^0.1.30",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "@vue/component-compiler-utils": "^3.1.1",
    "clone": "^2.1.2",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.2",
    "deepmerge": "^4.2.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-vue": "^6.1.2",
    "fibers": "^4.0.2",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^4.0.2",
    "globby": "^11.0.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "json-schema-to-typescript": "^8.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "trash-cli": "^3.0.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-hot-reload-api": "^2.3.4",
    "vue-loader": "^15.8.3",
    "vue-property-decorator": "^8.3.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "vuetify": "^2.2.5",
    "vuetify-loader": "^1.4.3",
    "vuex": "^3.1.2",
    "vuex-class": "^0.3.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-livereload-plugin": "^2.2.0"
  },
  "nodecg": {
    "compatibleRange": "^1.5.0",
    "bundleDependencies": {
      "nodecg-speedcontrol": "^2.0.0"
    },
    "graphics": [
      {
        "file": "16x9-1p-sample.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "4x3-1p-1c-sample.html",
        "width": 1920,
        "height": 1080
      }
    ]
  }
}
